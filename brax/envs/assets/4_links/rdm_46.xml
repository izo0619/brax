<mujoco model="morphology">
  <option timestep="0.01"/>
  <default>
    <default class="/"/>
  </default>
  <asset>
    <texture name="texplane" type="2d" builtin="checker" rgb1="0.1 0.2 0.3" rgb2="0.2 0.3 0.4" width="300" height="300"/>
    <material name="MatPlane" class="/" texture="texplane" texrepeat="20 20" specular="1" shininess="1" reflectance="0.5"/>
  </asset>
  <worldbody>
    <light name="//unnamed_light_0" class="/" directional="true" pos="0 0 1.5" dir="0 0 -1.3" cutoff="1e+02" exponent="1" diffuse="1 1 1" specular="0.1 0.1 0.1"/>
    <geom name="ground" class="/" type="plane" size="15 15 0.5" material="MatPlane" rgba="1 1 1 1"/>
    <body name="body" pos="0 0 0.1">
      <geom name="geom0" class="/" type="capsule" contype="1" conaffinity="1" condim="3" size="0.05" rgba="0.028 0.85 0.03 1" friction="0.4 0.1 0.1" fromto="0 0 0 -0.14 0 0.62"/>
      <body name="body1" pos="-0.031 0 0.13">
        <geom name="geom1" class="/" type="capsule" contype="1" conaffinity="1" condim="3" size="0.044" rgba="0.36 0.5 0.4 1" friction="0.4 0.1 0.1" fromto="0 0 0 0.69 0 0.16"/>
        <body name="body2" pos="0.64 0 0.15">
          <geom name="geom2" class="/" type="capsule" contype="1" conaffinity="1" condim="3" size="0.049" rgba="0.46 0.84 0.99 1" friction="0.4 0.1 0.1" fromto="0 0 0 -0.15 0 0.74"/>
          <body name="body3" pos="-0.13 0 0.66">
            <geom name="geom3" class="/" type="capsule" contype="1" conaffinity="1" condim="3" size="0.051" rgba="0.15 0.96 0.95 1" friction="0.4 0.1 0.1" fromto="0 0 0 -0.45 0 -0.066"/>
            <joint name="joint3" class="/" type="hinge" axis="0 1 0" limited="true" stiffness="20" range="-64 64" armature="0.1" damping="2"/>
          </body>
          <joint name="joint2" class="/" type="hinge" axis="0 1 0" limited="true" stiffness="20" range="-66 66" armature="0.1" damping="2"/>
        </body>
        <joint name="joint1" class="/" type="hinge" axis="0 1 0" limited="true" stiffness="20" range="-60 60" armature="0.1" damping="2"/>
      </body>
      <joint name="rootx" class="/" type="slide" axis="1 0 0" limited="false" stiffness="0" armature="0" damping="0"/>
      <joint name="rootz" class="/" type="slide" axis="0 0 1" limited="false" stiffness="0" armature="0" damping="0"/>
      <joint name="rooty" class="/" type="hinge" axis="0 1 0" limited="false" stiffness="0" armature="0" damping="0"/>
    </body>
  </worldbody>
  <actuator>
    <motor name="motor3" class="/" ctrllimited="true" ctrlrange="-1 1" gear="1.4e+02" joint="joint3"/>
    <motor name="motor2" class="/" ctrllimited="true" ctrlrange="-1 1" gear="1.8e+02" joint="joint2"/>
    <motor name="motor1" class="/" ctrllimited="true" ctrlrange="-1 1" gear="1.3e+02" joint="joint1"/>
  </actuator>
  <sensor>
    <subtreelinvel name="velocity" body="body"/>
  </sensor>
</mujoco>
