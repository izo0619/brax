    <body name="body" pos="0 0 0.1">
      <geom name="geom0" class="/" type="capsule" contype="1" conaffinity="1" condim="3" size="0.044" rgba="0.84 0.68 0.44 1" friction="0.4 0.1 0.1" fromto="0 0 0 0.18 0 0.62"/>
      <body name="body1" pos="0.054 0 0.19">
        <geom name="geom1" class="/" type="capsule" contype="1" conaffinity="1" condim="3" size="0.059" rgba="0.8 0.12 0.9 1" friction="0.4 0.1 0.1" fromto="0 0 0 0.38 0 -0.0068"/>
        <joint name="joint1" class="/" type="hinge" axis="0 1 0" limited="true" stiffness="20" range="-31 31" armature="0.1" damping="2"/>
      </body>
      <body name="body2" pos="0.15 0 0.54">
        <geom name="geom2" class="/" type="capsule" contype="1" conaffinity="1" condim="3" size="0.052" rgba="0.27 0.062 0.23 1" friction="0.4 0.1 0.1" fromto="0 0 0 -0.72 0 0.22"/>
        <body name="body3" pos="-0.67 0 0.2">
