          <joint name="joint3" class="/" type="hinge" axis="0 1 0" limited="true" stiffness="20" range="-45 45" armature="0.1" damping="2"/>
        </body>
        <joint name="joint2" class="/" type="hinge" axis="0 1 0" limited="true" stiffness="20" range="-57 57" armature="0.1" damping="2"/>
      </body>
      <joint name="rootx" class="/" type="slide" axis="1 0 0" limited="false" stiffness="0" armature="0" damping="0"/>
      <joint name="rootz" class="/" type="slide" axis="0 0 1" limited="false" stiffness="0" armature="0" damping="0"/>
      <joint name="rooty" class="/" type="hinge" axis="0 1 0" limited="false" stiffness="0" armature="0" damping="0"/>
    </body>
  </worldbody>
  <actuator>
    <motor name="motor1" class="/" ctrllimited="true" ctrlrange="-1 1" gear="1.2e+02" joint="joint1"/>
    <motor name="motor3" class="/" ctrllimited="true" ctrlrange="-1 1" gear="1.5e+02" joint="joint3"/>
    <motor name="motor2" class="/" ctrllimited="true" ctrlrange="-1 1" gear="1.1e+02" joint="joint2"/>
  </actuator>
  <sensor>
    <subtreelinvel name="velocity" body="body"/>
  </sensor>
</mujoco>
