<mujoco model="morphology">
  <option timestep="0.01"/>
  <default>
    <default class="/"/>
  </default>
  <asset>
    <texture name="texplane" type="2d" builtin="checker" rgb1="0.1 0.2 0.3" rgb2="0.2 0.3 0.4" width="300" height="300"/>
    <material name="MatPlane" class="/" texture="texplane" texrepeat="20 20" specular="1" shininess="1" reflectance="0.5"/>
  </asset>
  <worldbody>
    <light name="//unnamed_light_0" class="/" directional="true" pos="0 0 1.5" dir="0 0 -1.3" cutoff="1e+02" exponent="1" diffuse="1 1 1" specular="0.1 0.1 0.1"/>
    <geom name="ground" class="/" type="plane" size="15 15 0.5" material="MatPlane" rgba="1 1 1 1"/>
    <body name="body" pos="0 0 0.99">
      <geom name="geom0" class="/" type="capsule" contype="1" conaffinity="1" condim="3" size="0.044" rgba="0.44 0.058 0.75 1" friction="0.4 0.1 0.1" fromto="0 0 0 -0.13 0 -0.25"/>
      <body name="body1" pos="-0.07 0 -0.13">
        <geom name="geom1" class="/" type="capsule" contype="1" conaffinity="1" condim="3" size="0.054" rgba="0.094 0.45 0.81 1" friction="0.4 0.1 0.1" fromto="0 0 0 -0.7 0 -0.18"/>
        <body name="body2" pos="-0.26 0 -0.069">
          <geom name="geom2" class="/" type="capsule" contype="1" conaffinity="1" condim="3" size="0.063" rgba="0.44 0.52 0.14 1" friction="0.4 0.1 0.1" fromto="0 0 0 0.16 0 -0.69"/>
          <body name="body3" pos="0.071 0 -0.3">
            <geom name="geom3" class="/" type="capsule" contype="1" conaffinity="1" condim="3" size="0.065" rgba="0.19 0.22 0.58 1" friction="0.4 0.1 0.1" fromto="0 0 0 0.55 0 0.48"/>
            <joint name="joint3" class="/" type="hinge" axis="0 1 0" limited="true" stiffness="20" range="-32 32" armature="0.1" damping="2"/>
          </body>
          <joint name="joint2" class="/" type="hinge" axis="0 1 0" limited="true" stiffness="20" range="-65 65" armature="0.1" damping="2"/>
        </body>
        <joint name="joint1" class="/" type="hinge" axis="0 1 0" limited="true" stiffness="20" range="-62 62" armature="0.1" damping="2"/>
      </body>
      <joint name="rootx" class="/" type="slide" axis="1 0 0" limited="false" stiffness="0" armature="0" damping="0"/>
      <joint name="rootz" class="/" type="slide" axis="0 0 1" limited="false" stiffness="0" armature="0" damping="0"/>
      <joint name="rooty" class="/" type="hinge" axis="0 1 0" limited="false" stiffness="0" armature="0" damping="0"/>
    </body>
  </worldbody>
  <actuator>
    <motor name="motor3" class="/" ctrllimited="true" ctrlrange="-1 1" gear="1.2e+02" joint="joint3"/>
    <motor name="motor2" class="/" ctrllimited="true" ctrlrange="-1 1" gear="1.6e+02" joint="joint2"/>
    <motor name="motor1" class="/" ctrllimited="true" ctrlrange="-1 1" gear="1.1e+02" joint="joint1"/>
  </actuator>
  <sensor>
    <subtreelinvel name="velocity" body="body"/>
  </sensor>
</mujoco>
