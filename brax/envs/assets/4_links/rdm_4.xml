<mujoco model="morphology">
  <option timestep="0.01"/>
  <default>
    <default class="/"/>
  </default>
  <asset>
    <texture name="texplane" type="2d" builtin="checker" rgb1="0.1 0.2 0.3" rgb2="0.2 0.3 0.4" width="300" height="300"/>
    <material name="MatPlane" class="/" texture="texplane" texrepeat="20 20" specular="1" shininess="1" reflectance="0.5"/>
  </asset>
  <worldbody>
    <light name="//unnamed_light_0" class="/" directional="true" pos="0 0 1.5" dir="0 0 -1.3" cutoff="1e+02" exponent="1" diffuse="1 1 1" specular="0.1 0.1 0.1"/>
    <geom name="ground" class="/" type="plane" size="15 15 0.5" material="MatPlane" rgba="1 1 1 1"/>
    <body name="body" pos="0 0 0.5">
      <geom name="geom0" class="/" type="capsule" contype="1" conaffinity="1" condim="3" size="0.061" rgba="0.99 0.94 0.23 1" friction="0.4 0.1 0.1" fromto="0 0 0 0.2 0 -0.4"/>
      <body name="body1" pos="0.014 0 -0.027">
        <geom name="geom1" class="/" type="capsule" contype="1" conaffinity="1" condim="3" size="0.05" rgba="0.89 0.38 0.49 1" friction="0.4 0.1 0.1" fromto="0 0 0 0.67 0 -0.18"/>
        <body name="body2" pos="0.63 0 -0.17">
          <geom name="geom2" class="/" type="capsule" contype="1" conaffinity="1" condim="3" size="0.067" rgba="0.046 0.23 0.71 1" friction="0.4 0.1 0.1" fromto="0 0 0 -0.13 0 0.67"/>
          <body name="body3" pos="-0.054 0 0.29">
            <geom name="geom3" class="/" type="capsule" contype="1" conaffinity="1" condim="3" size="0.053" rgba="0.48 0.096 0.54 1" friction="0.4 0.1 0.1" fromto="0 0 0 -0.66 0 0.0021"/>
            <joint name="joint3" class="/" type="hinge" axis="0 1 0" limited="true" stiffness="20" range="-44 44" armature="0.1" damping="2"/>
          </body>
          <joint name="joint2" class="/" type="hinge" axis="0 1 0" limited="true" stiffness="20" range="-60 60" armature="0.1" damping="2"/>
        </body>
        <joint name="joint1" class="/" type="hinge" axis="0 1 0" limited="true" stiffness="20" range="-39 39" armature="0.1" damping="2"/>
      </body>
      <joint name="rootx" class="/" type="slide" axis="1 0 0" limited="false" stiffness="0" armature="0" damping="0"/>
      <joint name="rootz" class="/" type="slide" axis="0 0 1" limited="false" stiffness="0" armature="0" damping="0"/>
      <joint name="rooty" class="/" type="hinge" axis="0 1 0" limited="false" stiffness="0" armature="0" damping="0"/>
    </body>
  </worldbody>
  <actuator>
    <motor name="motor3" class="/" ctrllimited="true" ctrlrange="-1 1" gear="1.4e+02" joint="joint3"/>
    <motor name="motor2" class="/" ctrllimited="true" ctrlrange="-1 1" gear="1.7e+02" joint="joint2"/>
    <motor name="motor1" class="/" ctrllimited="true" ctrlrange="-1 1" gear="1e+02" joint="joint1"/>
  </actuator>
  <sensor>
    <subtreelinvel name="velocity" body="body"/>
  </sensor>
</mujoco>
