  </asset>
  <worldbody>
    <light name="//unnamed_light_0" class="/" directional="true" pos="0 0 1.5" dir="0 0 -1.3" cutoff="1e+02" exponent="1" diffuse="1 1 1" specular="0.1 0.1 0.1"/>
    <geom name="ground" class="/" type="plane" size="15 15 0.5" material="MatPlane" rgba="1 1 1 1"/>
    <body name="body" pos="0 0 0.1">
      <geom name="geom0" class="/" type="capsule" contype="1" conaffinity="1" condim="3" size="0.059" rgba="0.43 0.51 0.61 1" friction="0.4 0.1 0.1" fromto="0 0 0 0.26 0 0.39"/>
      <body name="body1" pos="0.18 0 0.26">
        <geom name="geom1" class="/" type="capsule" contype="1" conaffinity="1" condim="3" size="0.054" rgba="0.55 0.83 0.29 1" friction="0.4 0.1 0.1" fromto="0 0 0 -0.17 0 0.11"/>
        <body name="body2" pos="-0.086 0 0.057">
          <geom name="geom2" class="/" type="capsule" contype="1" conaffinity="1" condim="3" size="0.04" rgba="0.04 0.39 0.86 1" friction="0.4 0.1 0.1" fromto="0 0 0 0.14 0 0.21"/>
          <body name="body3" pos="0.13 0 0.2">
            <geom name="geom3" class="/" type="capsule" contype="1" conaffinity="1" condim="3" size="0.065" rgba="0.33 0.94 0.8 1" friction="0.4 0.1 0.1" fromto="0 0 0 -0.73 0 0.49"/>
            <joint name="joint3" class="/" type="hinge" axis="0 1 0" limited="true" stiffness="20" range="-30 30" armature="0.1" damping="2"/>
          </body>
          <joint name="joint2" class="/" type="hinge" axis="0 1 0" limited="true" stiffness="20" range="-46 46" armature="0.1" damping="2"/>
        </body>
        <joint name="joint1" class="/" type="hinge" axis="0 1 0" limited="true" stiffness="20" range="-60 60" armature="0.1" damping="2"/>
      </body>
      <joint name="rootx" class="/" type="slide" axis="1 0 0" limited="false" stiffness="0" armature="0" damping="0"/>
      <joint name="rootz" class="/" type="slide" axis="0 0 1" limited="false" stiffness="0" armature="0" damping="0"/>
      <joint name="rooty" class="/" type="hinge" axis="0 1 0" limited="false" stiffness="0" armature="0" damping="0"/>
    </body>
  </worldbody>
  <actuator>
    <motor name="motor3" class="/" ctrllimited="true" ctrlrange="-1 1" gear="1.8e+02" joint="joint3"/>
    <motor name="motor2" class="/" ctrllimited="true" ctrlrange="-1 1" gear="1.6e+02" joint="joint2"/>
    <motor name="motor1" class="/" ctrllimited="true" ctrlrange="-1 1" gear="1.6e+02" joint="joint1"/>
  </actuator>
  <sensor>
    <subtreelinvel name="velocity" body="body"/>
  </sensor>
</mujoco>
