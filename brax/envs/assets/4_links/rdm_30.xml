<mujoco model="morphology">
  <option timestep="0.01"/>
  <default>
    <default class="/"/>
  </default>
  <asset>
    <texture name="texplane" type="2d" builtin="checker" rgb1="0.1 0.2 0.3" rgb2="0.2 0.3 0.4" width="300" height="300"/>
    <material name="MatPlane" class="/" texture="texplane" texrepeat="20 20" specular="1" shininess="1" reflectance="0.5"/>
  </asset>
  <worldbody>
    <light name="//unnamed_light_0" class="/" directional="true" pos="0 0 1.5" dir="0 0 -1.3" cutoff="1e+02" exponent="1" diffuse="1 1 1" specular="0.1 0.1 0.1"/>
    <geom name="ground" class="/" type="plane" size="15 15 0.5" material="MatPlane" rgba="1 1 1 1"/>
    <body name="body" pos="0 0 0.25">
      <geom name="geom0" class="/" type="capsule" contype="1" conaffinity="1" condim="3" size="0.069" rgba="0.27 0.59 0.29 1" friction="0.4 0.1 0.1" fromto="0 0 0 0.55 0 0.26"/>
      <body name="body1" pos="0.55 0 0.26">
        <geom name="geom1" class="/" type="capsule" contype="1" conaffinity="1" condim="3" size="0.043" rgba="0.75 0.96 0.1 1" friction="0.4 0.1 0.1" fromto="0 0 0 -0.39 0 0.72"/>
        <body name="body2" pos="-0.14 0 0.26">
          <geom name="geom2" class="/" type="capsule" contype="1" conaffinity="1" condim="3" size="0.066" rgba="0.23 0.73 0.15 1" friction="0.4 0.1 0.1" fromto="0 0 0 -0.72 0 -0.43"/>
          <body name="body3" pos="-0.71 0 -0.42">
            <geom name="geom3" class="/" type="capsule" contype="1" conaffinity="1" condim="3" size="0.056" rgba="0.94 0.44 0.18 1" friction="0.4 0.1 0.1" fromto="0 0 0 0.17 0 -0.25"/>
            <joint name="joint3" class="/" type="hinge" axis="0 1 0" limited="true" stiffness="20" range="-59 59" armature="0.1" damping="2"/>
          </body>
          <joint name="joint2" class="/" type="hinge" axis="0 1 0" limited="true" stiffness="20" range="-60 60" armature="0.1" damping="2"/>
        </body>
        <joint name="joint1" class="/" type="hinge" axis="0 1 0" limited="true" stiffness="20" range="-37 37" armature="0.1" damping="2"/>
      </body>
      <joint name="rootx" class="/" type="slide" axis="1 0 0" limited="false" stiffness="0" armature="0" damping="0"/>
      <joint name="rootz" class="/" type="slide" axis="0 0 1" limited="false" stiffness="0" armature="0" damping="0"/>
      <joint name="rooty" class="/" type="hinge" axis="0 1 0" limited="false" stiffness="0" armature="0" damping="0"/>
    </body>
  </worldbody>
  <actuator>
    <motor name="motor3" class="/" ctrllimited="true" ctrlrange="-1 1" gear="1.1e+02" joint="joint3"/>
    <motor name="motor2" class="/" ctrllimited="true" ctrlrange="-1 1" gear="1e+02" joint="joint2"/>
    <motor name="motor1" class="/" ctrllimited="true" ctrlrange="-1 1" gear="1.5e+02" joint="joint1"/>
  </actuator>
  <sensor>
    <subtreelinvel name="velocity" body="body"/>
  </sensor>
</mujoco>
