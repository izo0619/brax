        <joint name="joint1" class="/" type="hinge" axis="0 1 0" limited="true" stiffness="1e+02" range="-40 40" armature="0.2" damping="3.5"/>
      </body>
      <body name="body2" pos="-0.03 0 -0.096">
        <geom name="geom2" class="/" type="capsule" contype="1" conaffinity="1" condim="3" size="0.055" rgba="0.85 0.81 0.47 1" friction="0.6 0.1 0.1" fromto="0 0 0 0.65 0 0.21"/>
        <body name="body3" pos="0.2 0 0.064">
          <geom name="geom3" class="/" type="capsule" contype="1" conaffinity="1" condim="3" size="0.057" rgba="0.45 0.16 0.83 1" friction="0.6 0.1 0.1" fromto="0 0 0 -0.065 0 0.39"/>
          <body name="body4" pos="-0.062 0 0.37">
            <geom name="geom4" class="/" type="capsule" contype="1" conaffinity="1" condim="3" size="0.058" rgba="0.46 0.35 0.72 1" friction="0.6 0.1 0.1" fromto="0 0 0 -0.19 0 0.28"/>
            <joint name="joint4" class="/" type="hinge" axis="0 1 0" limited="true" stiffness="1e+02" range="-45 45" armature="0.2" damping="3.5"/>
          </body>
          <joint name="joint3" class="/" type="hinge" axis="0 1 0" limited="true" stiffness="1e+02" range="-43 43" armature="0.2" damping="3.5"/>
        </body>
        <joint name="joint2" class="/" type="hinge" axis="0 1 0" limited="true" stiffness="1e+02" range="-35 35" armature="0.2" damping="3.5"/>
