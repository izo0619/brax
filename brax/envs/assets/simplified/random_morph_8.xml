<mujoco model="morphology">
  <option timestep="0.01"/>
  <default>
    <default class="/"/>
  </default>
  <asset>
    <texture name="texplane" type="2d" builtin="checker" rgb1="0.1 0.2 0.3" rgb2="0.2 0.3 0.4" width="300" height="300"/>
