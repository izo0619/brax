    <body name="body" pos="0 0 0.8">
      <geom name="geom0" class="/" type="capsule" contype="1" conaffinity="1" condim="3" size="0.046" rgba="0.86 0.42 0.84 1" friction="0.6 0.1 0.1" fromto="0 0 0 -0.21 0 -0.68"/>
      <body name="body1" pos="-0.19 0 -0.61">
